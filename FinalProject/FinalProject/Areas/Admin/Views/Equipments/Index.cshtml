@model List<Equipments>
<div class="row">

    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Alətlər</h4>

                <div class="table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>

                                <th>
                                    Alətlərin adı
                                <th>
                                    Haqqında
                                </th>



                            </tr>
                        </thead>
                        <tbody>
                            @foreach (Equipments equipments in Model)
                            {
                                <tr>
                                    <td>
                                        @equipments.Name
                                    </td>
                                    <td>
                                        @equipments.Information
                                    </td>




                                </tr>
                            }


                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>
