@model Patients
@{
    ViewData["Title"] = "Detail";
}

<div class="row">

    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <div>Xəstə nömrəsi: @Model.PatientNumber.Number</div>
                <h4>Xəstə adı: @Model.Name</h4>
                <div>Xəstənin həkimi: @Model.Doctors</div>
                <a class="btn btn-primary my-3" asp-action="Index">Geriyə</a>
            </div>
        </div>
    </div>
</div>