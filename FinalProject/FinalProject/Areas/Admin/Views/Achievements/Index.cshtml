@model List<Achievements>
    <div class="row">

        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Nailiyyətlər</h4>

                    <div class="table-responsive">
                        <table class="table table-striped">
                            <thead>
                                <tr>

                                    <th>
                                        Nailiyyətlər
                                    <th>
                                        Haqqında
                                    </th>



                                </tr>
                            </thead>
                            <tbody>
                                @foreach (Achievements achievements in Model)
                                {
                                <tr>
                                    <td>
                                        @achievements.Image
                                    </td>
                                    <td>
                                        @achievements.Title
                                    </td>

                                    <td>
                                        @achievements.Information

                                    </td>

                                    @*<td>
            @branches.IsDeactive
        </td>*@


                                </tr>
                                }


                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

    </div>


