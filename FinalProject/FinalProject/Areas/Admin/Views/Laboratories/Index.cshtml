@model List<Laboratories>
<div class="row">

    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Laboratoriyalar</h4>

                <div class="table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>
                                   Laboratoriya növü
                                <th>
                                    Haqqında
                                </th>




                            </tr>
                        </thead>
                        <tbody>
                            @foreach (Laboratories laboratories in Model)
                            {
                                <tr>
                                    <td>
                                        @laboratories.Name
                                    </td>

                                    <td>
                                        @laboratories.Information

                                    </td>

                                    @*<td>
                                            @branches.IsDeactive
                                        </td>*@


                                </tr>
                            }


                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>

